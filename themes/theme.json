{
	"name": "Dark Brackets Theme",
	"colors": {
		"editor.background": "#1d1f21",
		"editor.foreground": "#bdbdbd",
		"editor.lineHighlightBackground": "#2f2f2f",
		"editor.selectionBackground": "#0050a0",
		"editor.findMatchHighlightBackground": "#f6a644",
		"editorOverviewRuler.findMatchForeground": "#121212"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": "comment",
			"settings": {
			  "foreground": "#767676"
			}
		  },
		  {
			"name": "String",
			"scope": "string",
			"settings": {
			  "foreground": "#E5B074"
			}
		  },
		  {
			"name": "Keyword",
			"scope": "keyword",
			"settings": {
			  "foreground": "#8FAEEB"
			}
		  },
		  {
			"name": "Keyword operator",
			"scope": "keyword.operator",
			"settings": {
			  "foreground": "#bdbdbd"
			}
		  },
		  {
			"name": "Storage",
			"scope": "storage",
			"settings": {
			  "foreground": "#8FAEEB"
			}
		  },
		  {
			"name": "Storage modifier",
			"scope": [
			  "storage.modifier.import",
			  "storage.modifier.package"
			],
			"settings": {
			  "foreground": "#bdbdbd"
			}
		  },
		  {
			"name": "Library function",
			"scope": "support.function",
			"settings": {
			  "foreground": "#8FAEEB"
			}
		  },
		  {
			"name": "Function argument",
			"scope": "variable.parameter",
			"settings": {
			  "foreground": "#daa912"
			}
		  },
		  {
			"name": "Number",
			"scope": "constant.numeric",
			"settings": {
			  "foreground": "#85a300"
			}
		  },
		  {
			"name": "Built-in constant",
			"scope": "constant.language",
			"settings": {
			  "foreground": "#d89333"
			}
		  },
		  {
			"name": "User-defined constant",
			"scope": "constant.other",
			"settings": {
			  "fontStyle": "italic",
			  "foreground": "#d89333"
			}
		  },
		  {
			"name": "HTML declaration",
			"scope": [
			  "text.html.basic meta.tag.sgml.html punctuation.definition.tag.html",
			  "text.html.basic meta.tag.sgml.html meta.tag.sgml.doctype.html",
			  "text.html.basic meta.tag.sgml.html meta.tag.sgml.doctype.html string.quoted.double.doctype.identifiers-and-DTDs.html"
			],
			"settings": {
			  "foreground": "#bdbdbd"
			}
		  },
		  {
			"name": "HTML character",
			"scope": [
			  "text.html.basic constant.character.entity.html",
			  "text.xml constant.character.entity.xml"
			],
			"settings": {
			  "foreground": "#d89333"
			}
		  },
		  {
			"name": "Tag name",
			"scope": "entity.name.tag",
			"settings": {
			  "foreground": "#8FAEEB"
			}
		  },
		  {
			"name": "Tag start/end",
			"scope": [
			  "punctuation.definition.tag.begin",
			  "punctuation.definition.tag.end",
			  "punctuation.definition.tag.html"
			],
			"settings": {
			  "foreground": "#8FAEEB"
			}
		  },
		  {
			"name": "Tag attribute",
			"scope": "entity.other.attribute-name",
			"settings": {
			  "foreground": "#85a300"
			}
		  },
		  {
			"name": "CSS selector",
			"scope": [
			  "meta.selector.css entity.name.tag.css",
			  "meta.selector.css entity.other.attribute-name.class.css",
			  "meta.selector.css entity.other.attribute-name.id.css",
			  "meta.selector.css meta.attribute-selector.css entity.other.attribute-name.attribute.css",
			  "meta.selector.css meta.attribute-selector.css string.unquoted.attribute-value.css"
			],
			"settings": {
			  "foreground": "#8FAEEB"
			}
		  },
		  {
			"name": "CSS selector punctuation",
			"scope": "meta.selector.css meta.attribute-selector.css punctuation.separator.operator.css",
			"settings": {
			  "foreground": "#bdbdbd"
			}
		  },
		  {
			"name": "CSS selector status",
			"scope": [
			  "meta.selector.css entity.other.attribute-name.pseudo-class.css",
			  "meta.selector.css entity.other.attribute-name.pseudo-element.css"
			],
			"settings": {
			  "foreground": "#bdbdbd"
			}
		  },
		  {
			"name": "CSS at-rule",
			"scope": [
			  "keyword.control.at-rule",
			  "meta.at-rule support.type.property-name"
			],
			"settings": {
			  "foreground": "#b77fdb"
			}
		  },
		  {
			"name": "CSS at-rule support constant",
			"scope": "meta.at-rule support.constant",
			"settings": {
			  "foreground": "#85a300"
			}
		  },
		  {
			"name": "CSS at-rule operator",
			"scope": "meta.at-rule keyword.operator",
			"settings": {
			  "foreground": "#8FAEEB"
			}
		  },
		  {
			"name": "CSS property-name keyword",
			"scope": "meta.property-name.css support.type.property-name.css",
			"settings": {
			  "foreground": "#b77fdb"
			}
		  },
		  {
			"name": "CSS property-value",
			"scope": "meta.property-value.css",
			"settings": {
			  "foreground": "#bdbdbd"
			}
		  },
		  {
			"name": "CSS property-value keyword",
			"scope": [
			  "meta.property-value.css support.constant",
			  "meta.property-value.css support.function",
			  "meta.property-value.css variable.parameter.misc.css",
			  "meta.property-value.css constant.other.color.rgb-value.css"
			],
			"settings": {
			  "fontStyle": "",
			  "foreground": "#d89333"
			}
		  },
		  {
			"name": "CSS property-value number",
			"scope": [
			  "meta.property-value.css constant.numeric.css",
			  "meta.property-value.css constant.numeric.css keyword.other.unit.css",
			  "meta.at-rule constant.numeric.css keyword.other.unit.css"
			],
			"settings": {
			  "foreground": "#85a300"
			}
		  },
		  {
			"name": "CSS color name",
			"scope": "meta.property-value.css support.constant.color.w3c-standard-color-name.css",
			"settings": {
			  "foreground": "#8FAEEB"
			}
		  },
		  {
			"name": "CSS punctuation",
			"scope": [
			  "punctuation.separator.key-value.css",
			  "punctuation.terminator.rule.css",
			  "punctuation.section.property-list.css"
			],
			"settings": {
			  "foreground": "#bdbdbd"
			}
		  },
		  {
			"name": "JS keywords 1",
			"scope": [
			  "variable.language.js",
			  "storage.type.js",
			  "storage.type.function.js",
			  "meta.function.json.js storage.type.function.js",
			  "meta.class.instance.constructor keyword.operator.new.js"
			],
			"settings": {
			  "foreground": "#8FAEEB"
			}
		  },
		  {
			"name": "JS keywords 2",
			"scope": [
			  "support.function.js",
			  "support.function.dom.js",
			  "support.constant.js",
			  "entity.name.function.js",
			  "meta.function.json.js variable.parameter.function.js"
			],
			"settings": {
			  "foreground": "#b77fdb"
			}
		  },
		  {
			"name": "XML declaration",
			"scope": [
			  "text.xml meta.tag.preprocessor.xml punctuation.definition.tag.begin.xml",
			  "text.xml meta.tag.preprocessor.xml entity.name.tag.xml",
			  "text.xml meta.tag.preprocessor.xml entity.other.attribute-name.xml",
			  "text.xml meta.tag.preprocessor.xml string.quoted.double.xml",
			  "text.xml meta.tag.sgml.doctype.xml punctuation.definition.tag.begin.xml",
			  "text.xml meta.tag.sgml.doctype.xml keyword.doctype.xml"
			],
			"settings": {
			  "foreground": "#bdbdbd"
			}
		  },
		  {
			"name": "LaTeX keywords",
			"scope": [
			  "text.tex.latex constant",
			  "text.tex.latex constant.other"
			],
			"settings": {
			  "fontStyle": "",
			  "foreground": "#8FAEEB"
			}
		  },
		  {
			"name": "LaTeX mathmode",
			"scope": "text.tex.latex string.other.math",
			"settings": {
			  "foreground": "#85a300"
			}
		  },
		  {
			"name": "LaTeX mathmode braces",
			"scope": [
			  "text.tex.latex string.other.math punctuation.section.group.tex",
			  "text.tex.latex string.other.math punctuation.definition.arguments"
			],
			"settings": {
			  "foreground": "#bdbdbd"
			}
		  },
		  {
			"name": "LaTeX section",
			"scope": "text.tex.latex meta.function.section.latex entity.name.section.latex",
			"settings": {
			  "foreground": "#b77fdb"
			}
		  },
		  {
			"name": "LaTeX environment",
			"scope": "text.tex.latex variable.parameter.function.latex",
			"settings": {
			  "fontStyle": "italic",
			  "foreground": "#d89333"
			}
		  },
		  {
			"name": "LaTeX reference",
			"scope": "text.tex.latex constant.other.reference",
			"settings": {
			  "fontStyle": "",
			  "foreground": "#d89333"
			}
		  },
		  {
			"name": "LaTeX package",
			"scope": "text.tex.latex meta.preamble.latex support.class.latex",
			"settings": {
			  "foreground": "#85a300"
			}
		  },
		  {
			"name": "C++ preprocessor",
			"scope": "source.c++ meta.preprocessor",
			"settings": {
			  "foreground": "#8FAEEB"
			}
		  },
		  {
			"name": "SublimeLinter Error",
			"scope": "sublimelinter.mark.error",
			"settings": {
			  "foreground": "#D02000"
			}
		  },
		  {
			"name": "SublimeLinter Gutter Mark",
			"scope": "sublimelinter.gutter-mark",
			"settings": {
			  "foreground": "#FFFFFF"
			}
		  },
		  {
			"name": "SublimeLinter Warning",
			"scope": "sublimelinter.mark.warning",
			"settings": {
			  "foreground": "#DDB700"
			}
		  },
		  {
			"name": "GitGutter deleted",
			"scope": "markup.deleted.git_gutter",
			"settings": {
			  "foreground": "#F92672"
			}
		  },
		  {
			"name": "GitGutter inserted",
			"scope": "markup.inserted.git_gutter",
			"settings": {
			  "foreground": "#A6E22E"
			}
		  },
		  {
			"name": "GitGutter changed",
			"scope": "markup.changed.git_gutter",
			"settings": {
			  "foreground": "#967EFB"
			}
		  },
		  {
			"name": "GitGutter ignored",
			"scope": "markup.ignored.git_gutter",
			"settings": {
			  "foreground": "#565656"
			}
		  },
		  {
			"name": "GitGutter untracked",
			"scope": "markup.untracked.git_gutter",
			"settings": {
			  "foreground": "#565656"
			}
		  },
		  {
			"name": "js ts this",
			"scope": [
			  "var.this",
			  "variable.language.this.js",
			  "variable.language.this.ts",
			  "variable.language.this.jsx",
			  "variable.language.this.tsx"
			],
			"settings": {
			  "foreground": "#8FAEEB"
			}
		  },
		  {
			"name": "support.type.property-name.scss",
			"scope": [
			  "support.type.property-name.scss",
			  "support.type.property-name.css"
			],
			"settings": {
			  "foreground": "#b77fdb"
			}
		  },
		  {
			"name": "support.constant.property-value.scss",
			"scope": [
			  "support.constant.property-value.scss",
			  "support.constant.property-value.css"
			],
			"settings": {
			  "foreground": "#d89333"
			}
		  },
		  {
			"name": "Source Js Keyword Operator Delete,source Js Keyword Operator In,source Js Keyword Operator Of,source Js Keyword Operator Instanceof,source Js Keyword Operator New,source Js Keyword Operator Typeof,source Js Keyword Operator Void",
			"scope": [
			  "keyword.operator.expression.delete",
			  "keyword.operator.expression.in",
			  "keyword.operator.expression.of",
			  "keyword.operator.expression.instanceof",
			  "keyword.operator.new",
			  "keyword.operator.expression.typeof",
			  "keyword.operator.expression.void"
			],
			"settings": {
			  "foreground": "#8FAEEB"
			}
		  },
		  {
			"name": "js template-expression",
			"scope": [
			  "punctuation.definition.template-expression.begin.js",
			  "punctuation.definition.template-expression.end.js"
			],
			"settings": {
			  "foreground": "#8FAEEB"
			}
		  },
		  {
			"name": "TemplateString",
			"scope": "meta.template.expression.js",
			"settings": {
			  "foreground": "#bdbdbd"
			}
		  },
		  {
			"name": "js module",
			"scope": [
			  "support.module.node.js",
			  "support.type.object.module.js",
			  "support.module.node.js"
			],
			"settings": {
			  "foreground": "#8FAEEB"
			}
		  },
		  {
			"name": "parameter function",
			"scope": "function.parameter",
			"settings": {
			  "foreground": "#fec514"
			}
		  },
		  {
			"name": "parameter function",
			"scope": "function.parameter",
			"settings": {
			  "foreground": "#fec514"
			}
		  },
		  {
			"name": "JSON Key",
			"scope": "support.type.property-name.json",
			"settings": {
			  "foreground": "#8FAEEB"
			}
		  }	
	]
}
